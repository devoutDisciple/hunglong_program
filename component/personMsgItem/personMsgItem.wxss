.msg_item {
    height: 100rpx;
    background: #fff;
    line-height: 100rpx;
    display: flex;
    flex-direction: row;
    padding: 0 20rpx;
    border-bottom: 1rpx solid #f8f8f8;
    font-size: 30rpx;
}

.msg_label {
    width: 200rpx;
    font-size: 30rpx;
    color: #333333;
}

.msg_value {
    flex: 1;
    text-align: right;
    font-size: 30rpx;
    color: #AAAAAA;
}

.msg_value image {
    height: 80rpx;
    width: 80rpx;
    margin-top: 15rpx;
    border-radius: 80rpx;
}

.msg_bg image {
    border-radius: 0;
    width: 140rpx;
}

.msg_icon {
    width: 50rpx;
    text-align: right;
    color: #acacac;
}

.msg_icon image {
    height: 30rpx;
    width: 30rpx;
}