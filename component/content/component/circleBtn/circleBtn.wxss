.circle {
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.img {
    height: 30rpx;
    width: 30rpx;
}
.txt{
    font-size: 22rpx;
    margin-left: 10rpx;
    max-width: 155rpx;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}