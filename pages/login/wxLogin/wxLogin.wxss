.container {
    height: 100vh;
    display: flex;
    flex-direction: column;
    font-size: 26rpx;
}
.content{
    flex: 1;
    width: 100%;
}
.privacy{
    height: 200rpx;
    width: 100%;
    text-align: center;
    line-height: 200rpx;
}
.privacy_txt{
    color: #399eff;
}
.logo {
    width: 100%;
    height: 300rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.logo image {
    height: 100rpx;
    height: 100rpx;
    border-radius: 100%;
}
.welcome{
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    font-size: 50rpx;
}
.req {
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
}
.btn {
    height: 14vw;
    margin-top: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}
.btn image {
    width: 80vw;
    height: 14vw;
    position: absolute;
    left: 10vw;
    top: 0;
}
.btn_txt {
    position: absolute;
    height: 14vw;
    width: 80vw;
    left: 10vw;
    top: 0;
    line-height: 14vw;
    text-align: center;
    color: #fff;
}
.btn_vx{
    position: absolute;
    height: 14vw;
    width: 80vw;
    left: 10vw;
    top: 0;
    opacity: 0;
}